{
    "name": "Gitweb",
    "id": "gitweb",
    "packaging_format": 1,
    "description": {
        "en": "Git web interface (web frontend to Git repositories)",
        "fr": "Interface Web Git"
    },
    "version": "2.34.1~ynh1",
    "url": "https://git-scm.com/",
    "upstream": {
        "license": "MIT",
        "website": "https://git-scm.com/",
        "demo": "https://demo.example.com",
        "admindoc": "https://git-scm.com/book/fr/v2/Git-sur-le-serveur-GitWeb",
        "userdoc": "https://git-scm.com/docs/gitweb",
        "code": "https://some.forge.com/example/example"
    },
    "license": "MIT",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": false,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
